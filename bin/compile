#!/usr/bin/env bash

set -euo pipefail

readonly BUILD_DIR=$1
readonly CLEAN_FILES="${BUILD_DIR}/target/luxapp-api-0.0.1-SNAPSHOT.jar.original"

if [[ ! -n "${BUILD_DIR}" ]]; then
  echo "No build directory specified - exiting"
  exit 1
fi

rm "${BUILD_DIR}/target/luxapp-api-0.0.1-SNAPSHOT.jar.original"
rm -rf "${BUILD_DIR}/src"
